# Copy to .env.local and adjust as needed (dev only).
# This profile disables inbound JWT validation.

# Stack selection
STACK_PROFILE=dev-insecure
STACK_VERSION=0.3.5

# Host ports
RUN_GATEWAY_HOST_PORT=8081
RUN_COORDINATOR_HOST_PORT=8082
KEYCLOAK_HOST_PORT=8080

# Keycloak admin (dev only)
KEYCLOAK_ADMIN=admin
KEYCLOAK_ADMIN_PASSWORD=admin

# Client secrets (must match compose/keycloak/realm-arp-dev.json)
# These are set to dev defaults of "arp-some-service-secret", and in actual deployment you need to generate your own unique secrets.
ARP_RUN_GATEWAY_CLIENT_SECRET=arp-run-gateway-secret
ARP_RUN_COORDINATOR_CLIENT_SECRET=arp-run-coordinator-secret
ARP_ATOMIC_EXECUTOR_CLIENT_SECRET=arp-atomic-executor-secret
ARP_COMPOSITE_EXECUTOR_CLIENT_SECRET=arp-composite-executor-secret
ARP_NODE_REGISTRY_CLIENT_SECRET=arp-node-registry-secret
ARP_SELECTION_CLIENT_SECRET=arp-selection-secret
ARP_PDP_CLIENT_SECRET=arp-pdp-secret
ARP_RUN_STORE_CLIENT_SECRET=arp-jarvis-runstore-secret
ARP_EVENT_STREAM_CLIENT_SECRET=arp-jarvis-eventstream-secret
ARP_ARTIFACT_STORE_CLIENT_SECRET=arp-jarvis-artifactstore-secret

# LLM settings (required for the default OpenAI profile)
ARP_LLM_API_KEY=
ARP_LLM_CHAT_MODEL=

# Dev CLI (auth login via device flow)
ARP_DEV_CLI_CLIENT_ID=arp-dev-cli
